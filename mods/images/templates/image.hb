<div class="image">
	<canvas width="100" height="100"></canvas>
</div>

<div class="imageMenu">

	<h3 class="label">{{label}}</h3>

	<ul>
		<li><a class="viewImage btn btn-small" href="#/image/{{id}}">View Image</a></li>
