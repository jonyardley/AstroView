  "bugs": {
    "url": "https://github.com/jonyardley/AstroView/issues"
  },
  "homepage": "https://github.com/jonyardley/AstroView",
  "napa": {
    "fitsjs": "astrojs/fitsjs",
    "fabric": "kangax/fabric.js"
  },
  "browser": {
    "fitsjs": "fitsjs/lib/fits"
  }
}
